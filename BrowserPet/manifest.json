{
    "manifest_version": 3,
    "name": "Browser Pet",
    "version": "1.1.0",
    
    "content_scripts": [
        {
            "js": ["addPet.js"],
            "matches": ["<all_urls>"],
			"run_at": "document_end"
        }
    ],
    "icons": {
        "16": "assests/16.png",
        "32": "assests/32.png",
        "48": "assests/48.png",
        "128": "assests/128.png"
    },

    "action" : {
        "default_title" : "Browser Pets",
        "default_icon" : "assests/16.png"
    },
    "description": "A Chrome Extension that adds cute pets to your browser.",
    "author": "Justin Park"
}